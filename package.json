{
  "private": true,
  "scripts": {
    "build": "lerna run build --stream",
    "debug": "ts-node --project ./packages/debug/tsconfig.json ./packages/debug/lib/debug.ts",
    "host:build": "lerna run --scope host --stream build",
    "host:flash": "lerna run --scope host --stream flash",
    "app:build": "lerna run --scope app --stream build",
    "app:start": "lerna run --scope app --stream flash",
    "start": "run-s app:build app:start"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "lerna": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "name": "js-macro"
}
