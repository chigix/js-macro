    "$(MODULES)/pins/digital/manifest.json",
    "$(MODULES)/pins/digital/monitor/manifest.json",
    "$(MODDABLE)/modules/network/ble/manifest_server.json",
    "$(MODULES)/base/modules/manifest.json"
  ],
  "creation": {
    "keys": {
      "available": 512
    }
  },
  "defines": {
